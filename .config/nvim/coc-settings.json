{
  "languageserver": {
		"terraform": {
			"command": "terraform-ls",
			"args": ["serve"],
			"filetypes": [
				"terraform",
				"tf"
			],
			"initializationOptions": {},
			"settings": {}
		},
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "cSpell.language": "en,ca",
  "cSpellExt.enableDictionaries": ["catalan"],
  "cSpell.dictionaryDefinitions": [
      { "name": "persons", "path": "/Users/akawamura/.local/config/cspell/persons.txt", "addWords": true },
      { "name": "tech", "path": "/Users/akawamura/.local/config/cspell/tech.txt", "addWords": true },
